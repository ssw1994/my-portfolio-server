{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "src/functions/main.ts", "outDir": "dist/functions" }
    ],
    "outputPath": "dist",
    "watchAssets": true
  }
}
